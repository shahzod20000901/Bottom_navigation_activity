<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.bottom_navigation_activity.ui.home.HomeFragment"
        android:label="@string/search"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.bottom_navigation_activity.ui.dashboard.DashboardFragment"
        android:label="@string/liked"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.bottom_navigation_activity.ui.notifications.NotificationsFragment"
        android:label="@string/ad"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/messag"
        android:name="com.example.bottom_navigation_activity.ui.messag.messag"
        android:label="message_fragment"
        tools:layout="@layout/messag_fragment" />
    <fragment
        android:id="@+id/profile"
        android:name="com.example.bottom_navigation_activity.ui.profile.profil"
        android:label="profile_fragment"
        tools:layout="@layout/profil_fragment" />
</navigation>